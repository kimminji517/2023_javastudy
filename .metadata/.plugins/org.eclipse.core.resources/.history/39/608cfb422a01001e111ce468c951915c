	public static void main(String[] args) {
		String pathname = "D/KMJ/ex01.txt";
		FileReader fr = null;
		BufferedReader br = null;
		try {
			fr = new FileReader(pathname);
			br = new BufferedReader(fr);
			
			String msg = null;
			while((msg))
		} catch (Exception e) {
		} finally {
			// TODO: handle finally clause
		}
	}
}
