package com.ict.edu;

public class Ex09_Array {
	public static void main(String[] args) {
		// 선언과 생성을 하나로
		// 자료형[][] 이름 = new 자료형[1차원 배열 수][1차원배열 안에 존재하는 배열의 크기];
		
		char[][] ch = new char[4][2];
		
		// 고정길이 일때만 가능
		ch[0][0] = 'A';
		ch[0][1] = 'a';
		
		ch[][]
				ch[][]
						ch[][]
								ch[][]
										ch[][]
												ch[][]
	}
}
